#!/bin/bash -e

#Â©keithhedger Fri 18 Mar 13:02:28 GMT 2022 kdhedger68713@gmail.com

find -iname "*.png" | while read file
	do

	if [ ! -L "$file" ];then
		echo Converting "$file" ...
#colourize
		convert "$file"  -fill "#000000" -modulate 173,0,300 "$file"
#resize
		convert "$file" -resize 256x256 "$file"
	fi
done
